{
  "name": "Interia - Call Result Processing",
  "nodes": [
    {
      "parameters": {
        "path": "{{ $parameter.path }}",
        "authentication": "none",
        "respond": "immediately",
        "options": {}
      },
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1.1,
      "position": [
        400,
        300
      ],
      "id": "webhookTriggerNode",
      "webhookId": "PLACEHOLDER_WEBHOOK_ID"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "{{ $json.body.message.type }}",
              "operation": "equals",
              "value2": "end-of-call-report"
            }
          ]
        }
      },
      "name": "Is End Event?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        620,
        300
      ],
      "id": "ifCallEndNode"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "{{ $json.body.message.call.endedReason }}",
              "operation": "notEquals",
              "value2": "unanswered"
            },
            {
              "value1": "{{ $json.body.message.call.endedReason }}",
              "operation": "notEquals",
              "value2": "failed"
            },
            {
              "value1": "{{ $json.body.message.call.endedReason }}",
              "operation": "notEquals",
              "value2": "error"
            }
          ]
        },
        "options": {
          "combineOperation": "all"
        }
      },
      "name": "If Call Successful?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        840,
        300
      ],
      "id": "ifCallSuccessfulNode"
    },
    {
      "parameters": {
        "authentication": "airtableApi",
        "operation": "update",
        "baseId": "YOUR_BASE_ID",
        "tableId": "Leads",
        "recordId": "{{ $json.body.message.call.metadata.leadId }}",
        "typecast": true,
        "fieldsUi": {
          "fieldValues": [
            {
              "fieldId": "Status",
              "fieldValue": "Called"
            },
            {
              "fieldId": "Summary",
              "fieldValue": "{{ $json.body.message.call.summary || '' }}"
            }
          ]
        }
      },
      "name": "Airtable (Update Status - Called)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        1060,
        200
      ],
      "id": "updateCalledNode",
      "credentials": {
        "airtableApi": {
          "id": "PLACEHOLDER_CREDENTIAL_ID",
          "name": "Airtable account"
        }
      }
    },
    {
      "parameters": {
        "authentication": "airtableApi",
        "operation": "get",
        "baseId": "YOUR_BASE_ID",
        "tableId": "Leads",
        "recordId": "{{ $json.body.message.call.metadata.leadId }}",
        "options": {}
      },
      "name": "Airtable (Get Lead Data)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        1060,
        400
      ],
      "id": "getLeadDataNode",
      "credentials": {
        "airtableApi": {
          "id": "PLACEHOLDER_CREDENTIAL_ID",
          "name": "Airtable account"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "number": [
            {
              "value1": "{{ $node['Airtable (Get Lead Data)'].json.fields.Attempt || 0 }}",
              "operation": "smaller",
              "value2": 1
            }
          ]
        }
      },
      "name": "Check Attempt Count (<1?)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        1280,
        400
      ],
      "id": "ifAttemptCountNode"
    },
    {
      "parameters": {
        "authentication": "airtableApi",
        "operation": "update",
        "baseId": "YOUR_BASE_ID",
        "tableId": "Leads",
        "recordId": "{{ $node['Airtable (Get Lead Data)'].json.id }}",
        "typecast": true,
        "fieldsUi": {
          "fieldValues": [
            {
              "fieldId": "Attempt",
              "fieldValue": "{{ ($node['Airtable (Get Lead Data)'].json.fields.Attempt || 0) + 1 }}"
            }
          ]
        }
      },
      "name": "Airtable (Increment Attempt)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        1500,
        350
      ],
      "id": "incrementAttemptNode",
      "credentials": {
        "airtableApi": {
          "id": "PLACEHOLDER_CREDENTIAL_ID",
          "name": "Airtable account"
        }
      }
    },
    {
      "parameters": {
        "time": 1,
        "unit": "minutes",
        "options": {}
      },
      "name": "Wait 1 minute",
      "type": "n8n-nodes-base.wait",
      "typeVersion": 1.1,
      "position": [
        1720,
        350
      ],
      "id": "waitNode"
    },
    {
      "parameters": {
        "authentication": "airtableApi",
        "operation": "update",
        "baseId": "YOUR_BASE_ID",
        "tableId": "Leads",
        "recordId": "{{ $node['Airtable (Get Lead Data)'].json.id }}",
        "typecast": true,
        "fieldsUi": {
          "fieldValues": [
            {
              "fieldId": "Status",
              "fieldValue": "TBC"
            }
          ]
        }
      },
      "name": "Airtable (Set Status to TBC)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        1940,
        350
      ],
      "id": "updateTbcNode",
      "credentials": {
        "airtableApi": {
          "id": "PLACEHOLDER_CREDENTIAL_ID",
          "name": "Airtable account"
        }
      }
    },
    {
      "parameters": {
        "authentication": "airtableApi",
        "operation": "update",
        "baseId": "YOUR_BASE_ID",
        "tableId": "Leads",
        "recordId": "{{ $node['Airtable (Get Lead Data)'].json.id }}",
        "typecast": true,
        "fieldsUi": {
          "fieldValues": [
            {
              "fieldId": "Status",
              "fieldValue": "Failed"
            },
            {
              "fieldId": "FailReason",
              "fieldValue": "{{ $node['Webhook Trigger'].json.body.message.call.endedReason || 'Unreachable' }}"
            },
            {
              "fieldId": "Attempt",
              "fieldValue": "{{ ($node['Airtable (Get Lead Data)'].json.fields.Attempt || 0) + 1 }}"
            }
          ]
        }
      },
      "name": "Airtable (Update Status - Failed)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        1500,
        500
      ],
      "id": "updateFailedNode",
      "credentials": {
        "airtableApi": {
          "id": "PLACEHOLDER_CREDENTIAL_ID",
          "name": "Airtable account"
        }
      }
    },
    {
      "parameters": {
        "authentication": "airtableApi",
        "baseId": "YOUR_BASE_ID",
        "tableId": "Call Records",
        "operation": "create",
        "typecast": true,
        "fieldsUi": {
          "fieldValues": [
            {
              "fieldId": "LeadID",
              "fieldValue": "{{ '[\"' + $json.body.message.call.metadata.leadId + '\"]' }}"
            },
            {
              "fieldId": "callproviderID",
              "fieldValue": "{{ $json.body.message.call.id }}"
            },
            {
              "fieldId": "customernumber",
              "fieldValue": "{{ $json.body.message.call.customer.number }}"
            },
            {
              "fieldId": "type",
              "fieldValue": "outbound"
            },
            {
              "fieldId": "started",
              "fieldValue": "{{ new Date($json.body.message.call.startedAt).toISOString() }}"
            },
            {
              "fieldId": "ended",
              "fieldValue": "{{ new Date($json.body.message.call.endedAt).toISOString() }}"
            },
            {
              "fieldId": "milliseconds",
              "fieldValue": "{{ $json.body.message.call.endedAt - $json.body.message.call.startedAt }}"
            },
            {
              "fieldId": "ended reason",
              "fieldValue": "{{ $json.body.message.call.endedReason }}"
            },
            {
              "fieldId": "transcript",
              "fieldValue": "{{ $json.body.message.transcript || '' }}"
            }
          ]
        }
      },
      "name": "Airtable (Log Call Record)",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        1720,
        200
      ],
      "id": "logCallRecordNode",
      "credentials": {
        "airtableApi": {
          "id": "PLACEHOLDER_CREDENTIAL_ID",
          "name": "Airtable account"
        }
      }
    }
  ],
  "connections": { // VERIFY / RECREATE CONNECTIONS VISUALLY IN EDITOR
    "Webhook Trigger": {
      "main": [ [ { "node": "Is End Event?", "type": "main", "index": 0 } ] ]
    },
    "Is End Event?": {
      "main": [ [ { "node": "If Call Successful?", "type": "main", "index": 0 } ] ]
    },
    "If Call Successful?": {
      "main": [
        [ { "node": "Airtable (Update Status - Called)", "type": "main", "index": 0 } ], // True output
        [ { "node": "Airtable (Get Lead Data)", "type": "main", "index": 0 } ] // False output
      ]
    },
    "Airtable (Update Status - Called)": {
      "main": [ [ { "node": "Airtable (Log Call Record)", "type": "main", "index": 0 } ] ]
    },
    "Airtable (Get Lead Data)": {
      "main": [ [ { "node": "Check Attempt Count (<1?)", "type": "main", "index": 0 } ] ]
    },
    "Check Attempt Count (<1?)": {
      "main": [
        [ { "node": "Airtable (Increment Attempt)", "type": "main", "index": 0 } ], // True output
        [ { "node": "Airtable (Update Status - Failed)", "type": "main", "index": 0 } ] // False output
      ]
    },
    "Airtable (Increment Attempt)": {
      "main": [ [ { "node": "Wait 1 minute", "type": "main", "index": 0 } ] ]
    },
    "Wait 1 minute": {
      "main": [ [ { "node": "Airtable (Set Status to TBC)", "type": "main", "index": 0 } ] ]
    },
    "Airtable (Set Status to TBC)": {
      "main": [ [ { "node": "Airtable (Log Call Record)", "type": "main", "index": 0 } ] ]
    },
    "Airtable (Update Status - Failed)": {
      "main": [ [ { "node": "Airtable (Log Call Record)", "type": "main", "index": 0 } ] ]
    }
  },
  "settings": {},
  "staticData": null,
  "pinData": {},
  "versionId": "PLACEHOLDER_VERSION_ID",
  "triggerCount": 0,
  "active": false
}